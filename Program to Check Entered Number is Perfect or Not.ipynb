   "source": [
    "num = int(input('enter the number'))\n",
    "result = 0\n",
    "for i in range(1,num):\n",
    "    if num%i == 0:\n",
    "        result = result+i\n",
    "if result == num:\n",
    "    print(num , ' is a perfect number')\n",
    "else:\n",
    "    print(num ,' is not a perfect number')"
   ]
