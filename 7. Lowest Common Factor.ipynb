   "source": [
    "def lcm(a,b):\n",
    "    res = max(a,b)\n",
    "    while(True):\n",
    "        if(res%a==0 and res%b==0):\n",
    "            break\n",
    "        res += 1\n",
    "    return res\n",
    "def main():\n",
    "    a = int(input())\n",
    "    b = int(input())\n",
    "    print(lcm(a,b))\n",
    "\n",
    "if __name__=='__main__':\n",
    "    main()"
   ]
