    "def is_prime(n):\n",
    "    for i in range(2, (n//2)+1):\n",
    "        if(n%i==0):\n",
    "            return False\n",
    "    return True\n",
    "def main():\n",
    "    n = int(input())\n",
    "    print(is_prime(n))\n",
    "if __name__=='__main__':\n",
    "    main()"
   ]
