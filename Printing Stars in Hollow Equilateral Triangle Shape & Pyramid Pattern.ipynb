   "source": [
    "n = int(input('enter the number of rows'))\n",
    "for row in range(1,n+1):\n",
    "    for col in range(1, 2*n):\n",
    "        if row ==  n or row + col == n+1 or col - row == n-1:\n",
    "            print('*', end='')\n",
    "        else:\n",
    "            print(end=' ')\n",
    "    print()"
